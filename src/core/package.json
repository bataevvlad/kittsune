{
  "name": "@kittsune/core",
  "version": "2.0.0",
  "description": "Shared hooks and utilities for interaction state management",
  "main": "./lib/module/index.js",
  "types": "./index.ts",
  "react-native": "./index.ts",
  "source": "./index.ts",
  "exports": {
    ".": {
      "react-native": "./index.ts",
      "source": "./index.ts",
      "types": "./index.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "*.ts",
    "lib",
    "!**/__tests__",
    "!**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bataevvlad/kittsune.git"
  },
  "author": "Kitsune Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bataevvlad/kittsune/issues"
  },
  "homepage": "https://github.com/bataevvlad/kittsune#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "keywords": [
    "react-native",
    "hooks",
    "state-management"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bob build",
    "clean": "rm -rf lib"
  },
  "react-native-builder-bob": {
    "source": ".",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ]
    ]
  }
}
