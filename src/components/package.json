{
  "name": "@kittsune/components",
  "description": "React Native UI components based on Eva Design System",
  "version": "1.0.0",
  "main": "./lib/module/index.js",
  "types": "./index.ts",
  "react-native": "./index.ts",
  "source": "./index.ts",
  "exports": {
    ".": {
      "react-native": "./index.ts",
      "source": "./index.ts",
      "types": "./index.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "devsupport",
    "theme",
    "ui",
    "*.ts",
    "lib",
    "!**/__tests__",
    "!**/*.spec.*"
  ],
  "author": "Kitsune Contributors",
  "license": "MIT",
  "homepage": "https://github.com/bataevvlad/kitsune#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bataevvlad/kitsune.git"
  },
  "bugs": {
    "url": "https://github.com/bataevvlad/kitsune/issues"
  },
  "keywords": [
    "react-native",
    "kittsune",
    "ui-kit",
    "eva-design"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bob build",
    "clean": "rm -rf lib"
  },
  "dependencies": {
    "@kittsune/mapping-base": "^1.0.0",
    "@kittsune/processor": "^1.0.0",
    "fecha": "3.0.3",
    "hoist-non-react-statics": "^3.2.1"
  },
  "peerDependencies": {
    "@kittsune/core": "^1.0.0",
    "react": ">=18.2.0",
    "react-native": ">=0.72.0",
    "react-native-svg": ">=13.0.0"
  },
  "react-native-builder-bob": {
    "source": ".",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ]
    ]
  }
}
