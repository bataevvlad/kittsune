{
  "name": "@kittsune/processor",
  "description": "Eva Design System Schema Processor for Kittsune",
  "version": "2.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "author": "Kitsune Contributors",
  "homepage": "https://github.com/bataevvlad/kittsune#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bataevvlad/kittsune.git"
  },
  "bugs": {
    "url": "https://github.com/bataevvlad/kittsune/issues"
  },
  "keywords": [
    "kittsune",
    "eva",
    "processor",
    "schema",
    "react-native"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./dss": {
      "types": "./dss/index.d.ts",
      "default": "./dss/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "*.json",
    "*.map",
    "dss/",
    "js/",
    "!*.spec.*"
  ],
  "scripts": {
    "build": "echo 'Pre-compiled, no build needed'"
  },
  "publishConfig": {
    "access": "public"
  }
}
