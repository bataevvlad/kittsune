{
  "name": "kittsune",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@3.6.4",
  "license": "MIT",
  "author": "Kitsune Contributors",
  "homepage": "https://github.com/bataevvlad/kitsune#readme",
  "repository": "git+https://github.com/bataevvlad/kitsune.git",
  "bugs": {
    "url": "https://github.com/bataevvlad/kitsune/issues"
  },
  "workspaces": {
    "packages": [
      "src/*"
    ]
  },
  "scripts": {
    "build": "turbo run build --filter='./src/**'",
    "test": "jest --runInBand",
    "lint": "eslint -c .eslintrc.js ./src",
    "typecheck": "tsc --noEmit",
    "clean": "turbo run clean --filter='./src/**'",
    "changeset": "changeset",
    "version-packages": "changeset version && yarn install --no-immutable",
    "release": "yarn build && changeset publish"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-native": "0.81.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/react-native": "^13.0.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.12",
    "@types/lodash.merge": "^4.6.9",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "@types/rimraf": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-jest": "^29.7.0",
    "change-case": "^5.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-native-builder-bob": "^0.40.17",
    "react-native-svg": "15.12.1",
    "react-test-renderer": "19.1.0",
    "rimraf": "^5.0.0",
    "turbo": "^2.8.3",
    "typescript": "^5.3.0"
  },
  "resolutions": {
    "@types/react": "^19.0.0"
  }
}
