{
  "name": "kitsune",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Kitsune Contributors",
  "homepage": "https://github.com/bataevvlad/kitsune#readme",
  "repository": "git+https://github.com/bataevvlad/kitsune.git",
  "bugs": {
    "url": "https://github.com/bataevvlad/kitsune/issues"
  },
  "workspaces": {
    "packages": [
      "src/*",
      "docs"
    ]
  },
  "scripts": {
    "build": "gulp build",
    "clean": "rimraf ./dist",
    "lint": "eslint -c .eslintrc.js ./src",
    "test": "jest",
    "parse-docs": "gulp parse-docs",
    "view-docs": "cd src/showcases/ && npm run env -- dev && cd ../.. && yarn parse-docs && cd docs && yarn start && cd ..",
    "bump-version": "gulp bump-version && conventional-changelog -i CHANGELOG.md -s",
    "publish-docs": "npm run parse-docs && gulp publish-docs",
    "publish-packages": "gulp publish-packages",
    "r": "rm -rf node_modules && rm -rf src/showcases/node_modules && rm -rf docs/node_modules && yarn"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-native": "0.81.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/react-native": "^13.0.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/gulp": "^4.0.17",
    "@types/gulp-replace": "^0.0.31",
    "@types/jest": "^29.5.12",
    "@types/lodash.merge": "^4.6.9",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "@types/rimraf": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-jest": "^29.7.0",
    "change-case": "^5.4.0",
    "conventional-changelog-cli": "^4.1.0",
    "coveralls": "^3.1.1",
    "doc-prsr": "2.2.5",
    "eslint": "^8.57.0",
    "eslint-config-akveo": "^1.0.1",
    "eslint-plugin-react-native": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-typedoc": "^3.0.2",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-native-svg": "15.12.1",
    "react-test-renderer": "19.1.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0"
  },
  "resolutions": {
    "@types/react": "^19.0.0"
  }
}
